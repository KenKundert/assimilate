                            >     >         --repo=/{run_dir}/REPO/test \
                            >     >         '--match-archives=sh:{hostname}-{username}-test-*'

            ampere — delete {{{2:
                run: assimilate delete -a {ar1}
                checks:
                    stdout:
                        matches regex:
                            > Done. Run "borg compact" to free space.
                            >

                    ~/.local/share/assimilate/test.log:
